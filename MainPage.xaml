<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:Pileolinks"
            x:Class="Pileolinks.MainPage">
    <FlyoutPage.Flyout>
        <local:TreeFlyout x:Name="TreeFlyout" x:FieldModifier="Public"
            TreeItemViewModelChanged="Flyout_TreeItemViewModelChanged"
                          Loaded="TreeFlyout_Loaded"/>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage x:Name="TreeNavigation">
            <x:Arguments>
                <local:DirectoryView />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
    
</FlyoutPage>
